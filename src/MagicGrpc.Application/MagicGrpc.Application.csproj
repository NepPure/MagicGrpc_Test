<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>1.0.0.0</Version>
    <TargetFramework>net5.0</TargetFramework>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <AssemblyName>MagicGrpc.Application</AssemblyName>
    <PackageId>MagicGrpc.Application</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>MagicGrpc</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\MagicGrpc.Core\MagicGrpc.Core.csproj" />
    <ProjectReference Include="..\MagicGrpc.Shared\MagicGrpc.Shared.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Abp.EntityFrameworkCore" Version="6.5.0" />
    <PackageReference Include="Abp.AutoMapper" Version="6.5.0" />
    <PackageReference Include="MagicOnion.Server" Version="4.3.1" />
  </ItemGroup>
</Project>